<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zzt.viewpager2">

    <application
        android:name="debug.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.zzt.myviewpager.ActivityNestedSliding"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name="com.zzt.myviewpager.ActivityViewPager"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.zzt.myviewpager.ActivityViewPagerFragment"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name="com.zzt.myviewpager.ActivityViewPagerAdd"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name="com.zzt.myviewpager.ActivityViewPagerViewAdd"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
    </application>

</manifest>